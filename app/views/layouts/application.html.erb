<!DOCTYPE html>
<html>
	<head>
		<title><%= fullTitle(yield(:title)) %></title>
	
		<%= stylesheet_link_tag "application", media: "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
		<%= render :partial => "layouts/shim" %>
	</head>
	<body>
		<%= render :partial => "layouts/header" %>
		<div class="container">
			<%= yield %>
			<%= render :partial => "layouts/footer" %>
		</div>
	</body>
</html>
